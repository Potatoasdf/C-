    char identifier[3] = "hi";
    char publisher[80];
    int year = 0; 
    char title[80];
    float rating = 0;
    float duration = 0;

    void getYear(int& year)


    {
      cout << "Year?" << endl;
      cin >> year;
    }
    void getTitle(char* title) 
    {
      cout << "Title?" << endl;
