    {
      cout << "rating?" << endl;
      cin >> rating;
    }
    void getDuration(float& duration) 
    {
      cout << "Duration?" << endl;
      cin >> duration;
    }
    void getPublisher(char* publisher) 
    {
      cout << "Publisher?" << endl;
