#include <iostream>
#include <cstring>
#include "node.h"

using namespace std;

Node::Node(int newNum) {
  num = newNum;
  next = NULL;
}

Node::~Node() {
  delete num;
  next = NULL;
}

Node* Node::getNext() {
  return next;
}

Student* Node::getNum() {
  return num;
}

void Node::setNext(Node* newNext) {
  next = newNext;
}
